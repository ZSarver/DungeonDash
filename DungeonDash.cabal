name:                DungeonDash
version:             0.1.0.0
-- synopsis:            
-- description:         
license:             GPL-3
license-file:        LICENSE
author:              Alex Byaly, Zack!ary Sarver
maintainer:          abyaly@gmail.com
-- copyright:           
category:            Game
build-type:          Simple
-- extra-source-files:  
cabal-version:       >=1.10

executable DungeonDash
  main-is:             Main.hs
  -- other-modules:       
  -- other-extensions:    
  build-depends:       base >=4.7 && <4.8
                     , helm == 0.7.1
                     , elerea == 2.8.0
                     , random >= 1.0.1.1 && <2
                     , stm >= 2.4.2 && <3
                     , mtl == 2.1.3.1
                     , transformers == 0.3.0.0
                     , time == 1.4.2
                     , vector-space == 0.9
  hs-source-dirs:      src
  default-language:    Haskell2010